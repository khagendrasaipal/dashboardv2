<body style="background-color: rgba(0, 0, 0, 0.05)">


  <!-- Header Section Start -->
  <div class="col-md-12 text-center">
    <br>
    <img src="../Emblem_of_Nepal_2020.svg" style="width: 60px;"> 
    <h2>{{orgname}}</h2>
  </div>      
  <!-- Header Section End -->
  <br>
  
  
<!--   section Start -->
  
  

      	<div class="row d-flex justify-content-sm-center">
	
	<div class="col-md-2">
		<div style="width:100%">
			<div style="float:left">
				
				
				<img src="https://openweathermap.org/img/w/{{icon}}.png" style="height:2.5pc; width:2.5pc;">	
			</div>

			<div style="margin-left: 60px;">
				<span> {{temp}} &deg;C
					<br>
					{{type}} ({{desc}})
				</span>	
			</div>
		</div>
		
		
	</div>


	<div class="col-md-2">
		<div style="width:100%">
			<div style="float:left">
				<i class="bi bi-people" style="font-size:2.5pc; color:#ff00008c;"></i>	
			</div>

			<div style="margin-left: 60px;">
				<span> 
					{{census.get("population")}} <br>
					जनसंख्या

				</span>	
			</div>
		</div>
		
		
	</div>


	<div class="col-md-2">
		<div style="width:100%">
			<div style="float:left">
				<i class="bi bi-patch-exclamation" style="font-size:2.5pc; color: #1f00fb"></i>	
			</div>

			<div style="margin-left: 60px;">
				<span> 
					{{ward}} <br>
					वडा संख्या

				</span>	
			</div>
		</div>
		
		
	</div>

	<div class="col-md-2">
		<div style="width:100%">
			<div style="float:left">
				<i class="bi bi-hospital" style="font-size:2.5pc; color: #7d21cac7;"></i>	
			</div>

			<div style="margin-left: 60px;">
				<span> 
					{{hf}} <br>
					स्वास्थ्य संस्था संख्या

				</span>	
			</div>
		</div>
		
		
	</div>

	<div class="col-md-2">
		<div style="width:100%">
			<div style="float:left">
				<i class="bi bi-building" style="font-size:2.5pc;color: #1097cca6;"></i>	
			</div>

			<div style="margin-left: 60px;">
				<span>
					{{census.get("total_household")}} <br>
					घरधुरी संख्या

				</span>	
			</div>
		</div>
		
		
	</div>

</div>
  
  
