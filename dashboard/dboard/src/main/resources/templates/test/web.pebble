<div class="row col-md-12">
  <div class="col-md-6">
    <div class="card">
      <div class="card-body">
        <table class="table">
          <thead>
            <tr>
             
              <th scope="col">वडा</th>
              <th scope="col">जनसंख्या</th>
              <th scope="col">क्षेत्रफल(वर्ग कि.मी.)</th>
               
              
            </tr>
          </thead>
          <tbody>
           {% for i in basic %}
           <tr>
                <td>{{ i.get("ward")}}</td>
                <td>{{ i.get("population")}}</td>
                <td>{{ i.get("area_sqkm")}}</td>
				
		</tr> 
		 {% endfor %} 
          </tbody>
        </table></div>
      </div>
    </div>
    
    
    <div class="col-md-6">
    <div class="card">
      <div class="card-body">
        <table class="table">
          <thead>
            <tr>
             
              <th scope="col">वडा</th>
              <th scope="col">कुल स्वास्थ्य संस्था</th>
              <th scope="col">सरकारी  स्वास्थ्य संस्था</th>
               <th scope="col">गैरसरकारी स्वास्थ्य संस्था </th>
               <th scope="col">नर्मल बेड </th>
                <th scope="col">आइ सि यु बेड </th>
              <th scope="col">भेन्टिलेटर बेड </th>
              
            </tr>
          </thead>
          <tbody>
         
          {% for hf in hfcount %}
       
           <tr>
                
                <td>{{ hf.get("ward")}}</td>				
                <td>{{ hf.get("total")}}</td>
				<td>{{ hf.get("gov")}} </td>
				<td>{{ hf.get("ngov")}}</td>
				<td>{{ hf.get("normal")}}  </td>
				<td>{{ hf.get("icu")}}  </td>
				<td>{{ hf.get("vent")}} </td>
             
				
		</tr> 
		{% endfor %} 
          </tbody>
        </table></div>
      </div>
    </div>
  </div>
  <!-- row 5 end -->



